<Window x:Class="TennisClub.WpfDesktop.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TennisClub.WpfDesktop"
        mc:Ignorable="d"
        Icon="Images/tennis.ico"
        Title="Tennis club" Height="316" Width="459">
    <Grid>
        <Grid.Background>
            <LinearGradientBrush EndPoint="0,1" StartPoint="1,1">
                <GradientStop Color="#4caf50" Offset="0"/>
                <GradientStop Color="#4caf50" Offset="2"/>
            </LinearGradientBrush>
        </Grid.Background>
        <Frame
            x:Name="MainFrame"
            Loaded="MainFrame_Loaded"
            NavigationUIVisibility="Hidden"/>
    </Grid>
</Window>
